  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
      font-family: poppins, sans-serif;
    }

    header {
      position: relative;
      width: 100%;
      min-height: 95vh;
      background: rgb(40, 43, 46);
      display: flex;
      justify-content: center;
      align-items: center;
      overflow: hidden;
    }

    header:before {
      content: "";
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: rgb(76, 78, 80);
      clip-path: polygon(71% 0, 100% 0, 100% 100%, 93% 100%);
    }

    .nav {
      position: absolute;
      top: 75px;
      left: 50px;
      display: flex;
      border-left: 5px solid #000;
      padding-left: 20px;
    }


    .nav li {
      list-style: none;
    }

    .nav li a {
      color: white;
      text-decoration: none;
      margin-right: 30px;
      font-size: 0.9em;
      text-transform: uppercase;
      font-weight: 700;
    }

    .imgBox {
      position: relative;
      padding-right: 100px;
      padding-top: 40px;
    }

    .imgBox img {
      max-width: 300px;
    }

    .content {
      position: relative;
      padding: 100px 50px;
      box-sizing: border-box;
    }

    .content h2 {
      color: white;
      font-size: 1.5em;
      font-weight: 800;
      text-transform: uppercase;
    }

    .content p {
      color: white;
      font-size: 1.1em;

    }

    .buttons {
      display: flex;
      flex-wrap: wrap;
      align-items: center;
    }

    .buttons>button {
      margin: 5px;
    }

    .buttons a {
      position: relative;
      display: inline-block;
      background: #000;
      padding: 15px 30px;
      margin: 20px;
      margin-left: 10px;
      margin-right: 0;
      color: #fff;
      border: none;
      border-radius: 5PX;
      font-size: 0.95em;
      font-weight: 700;
      text-decoration: none;
      text-transform: uppercase;
      letter-spacing: 2px;

    }

    .social {
      position: absolute;
      bottom: 75px;
      left: 50%;
      transform: translateX(-50%);
      display: flex;

    }


    .social li {
      list-style: none;
    }

    .social li a {
      color: white;
      text-decoration: none;
      margin-right: 30px;
      font-size: 0.9em;
    }

    .social li a:hover {
      color: #fff;
    }

    @media only screen and (max-width: 500px) {
      header:before {
        clip-path: polygon(100% 50%, 100% 100%, 0% 100%);
      }

      .nav {
        position: absolute;
        top: 10px;
        left: 0px;
        width: 90%;
        display: flex;
        border-left: 5px solid #000;
        padding-left: 7px;
        margin: 10px;
      }

      .nav li {
        margin-right: 10px;
      }

      .nav li a {
        margin-right: 0px;

      }

      .imgBox {
        display: none;
      }

      .content {
        position: relative;
        padding: 0;
        box-sizing: border-box;
        width: 90%;
        margin: 20px;
      }

      .content h2 {
        color: white;
        font-size: 1.2rem;
        font-weight: 800;
        text-transform: uppercase;
      }

      .content p {
        color: white;
        font-size: 1em;

      }

      .buttons a {
        position: relative;
        display: inline-block;
        background: #000;
        padding: 10px;
        margin-top: 20px;
        margin-bottom: 0;
        color: #fff;
        border: none;
        border-radius: 5px;
        font-size: .7em;
        font-weight: 700;
        text-decoration: none;
        text-transform: uppercase;
        letter-spacing: 2px;

      }


    }

    .footer {
      height: 5vh;
      background-color: #17202A;
      color: #FDFEFE;
    }

    .center_col {
      display: flex;
      flex-flow: column;
      justify-content: center;
      align-items: center;
    }

    .center_col {
      display: flex;
      flex-flow: row;
      justify-content: center;
      align-items: center;
    }
  </style>


</head>

